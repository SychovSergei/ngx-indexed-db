{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2020",
    "paths": {
      "ngx-indexed-db": [
        "dist/ngx-indexed-db/ngx-indexed-db",
        "dist/ngx-indexed-db"
      ]
    },
    "useDefineForClassFields": false
  }
}
